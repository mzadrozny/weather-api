datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Property {
  id          String   @id @default(cuid())
  createdAt   DateTime @default(now())

  city        String
  street      String
  state       String
  zipCode     String

  lat         Float
  long        Float

  weatherData Json
}